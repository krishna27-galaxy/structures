#include <iostream>
using namespace std;
struct student{
    int rollnum;
    char sec;
    
}s[100];
int main()
{
   struct student *s1ptr;
   int n;
   cout<<"enter um of students";
   cin>>n;
   
   for(int i =0;i<n;i++)
   {
   cin>>s[i].rollnum;
   cin>>s[i].sec;
   
    }
    for(int i=0;i<n;i++)
    {
        cout<<"information for student  "<<i+1<<endl;
        cout<<" reg num: \t"<<s[i].rollnum<<"\t section :  "<<s[i].sec<<endl;
    }
    return 0;
}

